<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="450.0" prefWidth="850.0" styleClass="background" stylesheets="@DarkTheme.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.Clutchers.GUI.GestionRatingController">
   <children>
      <TableView fx:id="tvRating" layoutX="511.0" layoutY="95.0" onMouseClicked="#select" prefHeight="200.0" prefWidth="200.0">
        <columns>
          <TableColumn fx:id="tcRate" prefWidth="199.0" text="rate_index" />
        </columns>
      </TableView>
      <Label layoutX="107.0" layoutY="149.0" text="rate_index" />
      <TextField fx:id="tfRateindex" layoutX="219.0" layoutY="141.0" />
      <Button fx:id="btnUpdate" layoutX="97.0" layoutY="255.0" mnemonicParsing="false" onAction="#modifierRating" text="Update" />
      <Button fx:id="btnDelete" layoutX="196.0" layoutY="255.0" mnemonicParsing="false" onAction="#supprimerRating" text="Delete" />
      <Button fx:id="btnRetour" layoutX="333.0" layoutY="255.0" mnemonicParsing="false" onAction="#Retour" text="Retour" />
      <Label layoutX="62.0" layoutY="44.0" prefHeight="33.0" prefWidth="122.0" text="Gestion Rating">
         <font>
            <Font name="Arial Bold Italic" size="14.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
